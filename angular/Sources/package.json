{
  "name": "stroyka",
  "version": "1.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sprite": "svg-sprite --config src/sprite-config.json src/svg/*.svg",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
