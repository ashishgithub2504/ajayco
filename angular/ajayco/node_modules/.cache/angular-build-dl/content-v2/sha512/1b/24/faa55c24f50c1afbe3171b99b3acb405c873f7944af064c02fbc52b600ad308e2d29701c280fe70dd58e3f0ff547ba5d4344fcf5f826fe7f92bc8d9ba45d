            var CollapseContentDirective = /** @class */ (function () {
                function CollapseContentDirective(el, zone) {
                    this.el = el;
                    this.zone = zone;
                    this.destroy$ = new rxjs__WEBPACK_IMPORTED_MODULE_2__["Subject"]();
                }
