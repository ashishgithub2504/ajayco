            var MenuComponent = /** @class */ (function () {
                function MenuComponent() {
                    this.layout = 'classic';
                    this.items = [];
                    this.itemClick = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
                    this.hoveredItem = null;
                }
                MenuComponent.prototype.onItemMouseEnter = function (item) {
                    this.hoveredItem = item;
                };
                MenuComponent.prototype.onItemMouseLeave = function (item) {
                    if (this.hoveredItem === item) {
                        this.hoveredItem = null;
                    }
                };
                MenuComponent.prototype.onSubItemClick = function (item) {
                    this.hoveredItem = null;
                    this.itemClick.emit(item);
                };
                return MenuComponent;
            }());
