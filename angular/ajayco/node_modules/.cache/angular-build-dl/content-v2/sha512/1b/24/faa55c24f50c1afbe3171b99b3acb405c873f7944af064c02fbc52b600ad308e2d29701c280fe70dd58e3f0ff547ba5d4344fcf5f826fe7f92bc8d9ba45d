            /* harmony import */ var rxjs__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! rxjs */ "./node_modules/rxjs/_esm2015/index.js");
            /* harmony import */ var _angular_common_http__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/common/http */ "./node_modules/@angular/common/fesm2015/http.js");
            /* harmony import */ var util__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! util */ "./node_modules/util/util.js");
            /* harmony import */ var util__WEBPACK_IMPORTED_MODULE_3___default = /*#__PURE__*/ __webpack_require__.n(util__WEBPACK_IMPORTED_MODULE_3__);
            /* harmony import */ var _database_products__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ./database/products */ "./src/fake-server/database/products.ts");
            /* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! rxjs/operators */ "./node_modules/rxjs/_esm2015/operators/index.js");
            /* harmony import */ var _database_categories__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ./database/categories */ "./src/fake-server/database/categories.ts");
            /**
             * Returns products list.
             *
             * @param categorySlug Unique human-readable category identifier.
             * @param options Options list.
             */
            function getProductsList(categorySlug, options) {
                var page = options.page || 1;
                var limit = options.limit || 12;
                var sort = options.sort || 'default';
                var filterValues = options.filterValues || {};
                var filters = [];
                var filtersDef = [
                    { type: 'range', slug: 'price', name: 'Price' },
                    { type: 'check', slug: 'brand', name: 'Brand' },
                    { type: 'radio', slug: 'discount', name: 'With Discount' },
                    { type: 'color', slug: 'color', name: 'Color' },
                ];
                var items = _database_products__WEBPACK_IMPORTED_MODULE_4__["products"].slice();
                // Make filters.
                if (categorySlug === null) {
                    filters.push({
                        type: 'categories',
                        slug: 'categories',
                        name: 'Categories',
                        root: true,
                        items: _database_categories__WEBPACK_IMPORTED_MODULE_6__["shopCategoriesTree"].map(function (x) { return makeCategoryFilterItem('child', x); }).slice(),
                    });
                }
                else {
                    var category = _database_categories__WEBPACK_IMPORTED_MODULE_6__["shopCategoriesList"].find(function (x) { return x.slug === categorySlug; });
                    if (!category) {
                        return Object(rxjs__WEBPACK_IMPORTED_MODULE_1__["throwError"])(new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpErrorResponse"]({ status: 404, statusText: 'Page Not Found' }));
                    }
                    filters.push({
                        type: 'categories',
                        slug: 'categories',
                        name: 'Categories',
                        root: false,
                        items: category.parents.map(function (x) { return makeCategoryFilterItem('parent', x); }).concat([
                            makeCategoryFilterItem('current', category)
                        ], category.children.map(function (x) { return makeCategoryFilterItem('child', x); })),
                    });
                }
                makeFilters(filtersDef, items).forEach(function (x) { return filters.push(x); });
                // Apply values to filters.
                filters.forEach(function (filter) {
                    if (filter.slug in filterValues && 'value' in filter) {
                        filter.value = parseFilterValue(filter, filterValues[filter.slug]);
                    }
                });
                // Calculate items count for filter values.
                filters.forEach(function (filter) {
                    if (filter.type !== 'check' && filter.type !== 'color' && filter.type !== 'radio') {
                        return;
                    }
                    var counts = calcProductsForFilterValues(filter, filters, items);
                    filter.items.forEach(function (item) {
                        if (item.slug in counts) {
                            item.count = counts[item.slug];
                        }
                    });
                });
                // Apply filters to items list.
                items = items.filter(function (product) {
                    return filters.reduce(function (result, filter) { return result && testProduct(filter, product); }, true);
                });
                // Sort items array.
                items = items.sort(function (a, b) {
                    if (['name_asc', 'name_desc'].includes(sort)) {
                        if (a.name === b.name) {
                            return 0;
                        }
                        return (a.name > b.name ? 1 : -1) * (sort === 'name_asc' ? 1 : -1);
                    }
                    return 0;
                });
                // Preparing data for a response.
                var start = (page - 1) * limit;
                var end = start + limit;
                var total = items.length;
                var pages = Math.ceil(total / limit);
                var from = (page - 1) * limit + 1;
                var to = Math.max(Math.min(page * limit, total), from);
                items = items.slice(start, end);
                var response = {
                    items: items,
                    page: page,
                    limit: limit,
                    total: total,
                    pages: pages,
                    from: from,
                    to: to,
                    sort: sort,
                    filters: filters,
                    filterValues: filterValues,
                };
                return Object(rxjs__WEBPACK_IMPORTED_MODULE_1__["timer"])(350).pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_5__["map"])(function () { return JSON.parse(JSON.stringify(response)); }));
            }
            function getFilterValue(type, slug, product, defaultValue) {
                if (defaultValue === void 0) { defaultValue = null; }
                if (type === 'range' && slug === 'price') {
                    return product.price;
                }
                else if (type === 'check' && slug === 'brand') {
                    if (product.brand && typeof product.brand === 'object') {
                        return [{ slug: product.brand.slug, name: product.brand.name }];
                    }
                }
                else if (type === 'check' && slug === 'discount') {
                    var items = [
                        { slug: 'any', name: 'Any' },
                    ];
                    if (product.compareAtPrice) {
                        items.push({ slug: 'yes', name: 'Yes' });
                    }
                    else {
                        items.push({ slug: 'no', name: 'No' });
                    }
                    return items;
                }
                else if (type === 'check' || type === 'radio') {
                    if (!('attributes' in product) || !Object(util__WEBPACK_IMPORTED_MODULE_3__["isArray"])(product.attributes)) {
                        return defaultValue;
                    }
                    var attribute = product.attributes.find(function (x) { return x.slug === slug; });
                    if (!attribute) {
                        return defaultValue;
                    }
                    return attribute.values.map(function (x) { return ({ slug: x.slug, name: x.name }); });
                }
                return defaultValue;
            }
            function getRangeValue(slug, product, defaultValue) {
                if (defaultValue === void 0) { defaultValue = null; }
                return getFilterValue('range', slug, product, defaultValue);
            }
            function getListValues(slug, product, defaultValue) {
                if (defaultValue === void 0) { defaultValue = []; }
                return getFilterValue('check', slug, product, defaultValue);
            }
            function getColorCode(slug) {
                switch (slug) {
                    case 'white': return '#fff';
                    case 'silver': return '#d9d9d9';
                    case 'light-gray': return '#b3b3b3';
                    case 'gray': return '#808080';
                    case 'dark-gray': return '#666';
                    case 'coal': return '#4d4d4d';
                    case 'black': return '#262626';
                    case 'red': return '#ff4040';
                    case 'orange': return '#ff8126';
                    case 'yellow': return '#ffd333';
                    case 'pear-green': return '#becc1f';
                    case 'green': return '#8fcc14';
                    case 'emerald': return '#47cc5e';
                    case 'shamrock': return '#47cca0';
                    case 'shakespeare': return '#47cccc';
                    case 'blue': return '#40bfff';
                    case 'dark-blue': return '#3d6dcc';
                    case 'violet': return '#7766cc';
                    case 'purple': return '#b852cc';
                    case 'cerise': return '#e53981';
                }
                return '#000';
            }
            function parseFilterValue(filter, value) {
                switch (filter.type) {
                    case 'range':
                        return value.split('-').map(function (x) { return parseFloat(x); });
                    case 'check':
                    case 'color':
                        return value.trim() === '' ? [] : value.split(',').map(function (x) { return x.trim(); });
                }
                return value;
            }
            function testProduct(filter, product) {
                if (filter.type === 'range') {
                    var value = getRangeValue(filter.slug, product);
                    if (value === null || value < filter.value[0] || value > filter.value[1]) {
                        return false;
                    }
                }
                else if (filter.type === 'check' || filter.type === 'color') {
                    var values_1 = getListValues(filter.slug, product);
                    return filter.value.length < 1 || filter.value.reduce(function (isMatched, value) {
                        return isMatched || !!values_1.find(function (x) { return x.slug === value; });
                    }, false);
                }
                else if (filter.type === 'radio') {
                    var values = getListValues(filter.slug, product);
                    return !!values.find(function (x) { return x.slug === filter.value; });
                }
                return true;
            }
            function calcProductsForFilterValues(filter, allFilters, products) {
                var result = {};
                products = products.filter(function (product) { return allFilters.reduce(function (isMatched, eachFilter) {
                    return isMatched && (filter.slug === eachFilter.slug || testProduct(eachFilter, product));
                }, true); });
                products.forEach(function (product) {
                    switch (filter.type) {
                        case 'check':
                        case 'color':
                        case 'radio':
                            getListValues(filter.slug, product).forEach(function (value) {
                                if (!(value.slug in result)) {
                                    result[value.slug] = 0;
                                }
                                result[value.slug] += 1;
                            });
                            break;
                    }
                });
                return result;
            }
            function makeFilters(filtersDef, products) {
                var result = [];
                filtersDef.forEach(function (filterDef) {
                    if (filterDef.type === 'range') {
                        var max = products.reduce(function (value, product) { return Math.max(value, getRangeValue(filterDef.slug, product, value)); }, 0);
                        var min = products.reduce(function (value, product) { return Math.min(value, getRangeValue(filterDef.slug, product, value)); }, max);
                        /** Calculates the number of digits for rounding. */
                        var digit = Math.max(Math.ceil(max).toString().length - 2, 1);
                        digit = Math.pow(10, digit);
                        max = Math.ceil(max / digit) * digit;
                        min = Math.floor(min / digit) * digit;
                        result.push({
                            type: filterDef.type,
                            slug: filterDef.slug,
                            name: filterDef.name,
                            value: [min, max],
                            // options
                            min: min,
                            max: max,
                        });
                    }
                    else if (filterDef.type === 'check' || filterDef.type === 'radio' || filterDef.type === 'color') {
                        var itemsBySlug_1 = {};
                        var items_1 = [];
                        products.forEach(function (product) {
                            getListValues(filterDef.slug, product).forEach(function (value) {
                                if (value.slug in itemsBySlug_1) {
                                    return;
                                }
                                var item = makeFilterItem(filterDef.type, value);
                                itemsBySlug_1[value.slug] = item;
                                items_1.push(item);
                            });
                        });
                        if (items_1.length < 1 || (filterDef.type === 'radio' && items_1.length < 2)) {
                            return;
                        }
                        items_1 = sortFilterItems(filterDef.type, filterDef.slug, items_1);
                        result.push({
                            type: filterDef.type,
                            slug: filterDef.slug,
                            name: filterDef.name,
                            value: filterDef.type === 'radio' ? items_1[0].slug : [],
                            items: items_1,
                        });
                    }
                });
                return result;
            }
            function makeFilterItem(filterType, value) {
                switch (filterType) {
                    case 'check':
                    case 'radio':
                        return {
                            slug: value.slug,
                            name: value.name,
                            count: 0,
                        };
                    case 'color':
                        return {
                            slug: value.slug,
                            name: value.name,
                            count: 0,
                            color: getColorCode(value.slug),
                        };
                }
            }
            function makeCategoryFilterItem(type, category) {
                return {
                    slug: category.slug,
                    name: category.name,
                    type: type,
                    category: Object.assign({}, category, { children: null, parents: null }),
                    count: category.items,
                };
            }
            function sortFilterItems(filterType, filterSlug, items) {
                if (filterType === 'color' && filterSlug === 'color') {
                    var attributeDef = _database_products__WEBPACK_IMPORTED_MODULE_4__["attributesDef"].find(function (x) { return x.slug === filterSlug; });
                    if (attributeDef) {
                        var values_2 = attributeDef.values.map(function (x) { return x.slug; });
                        return items.sort(function (a, b) {
                            return values_2.indexOf(a.slug) - values_2.indexOf(b.slug);
                        });
                    }
                }
                return items;
            }
