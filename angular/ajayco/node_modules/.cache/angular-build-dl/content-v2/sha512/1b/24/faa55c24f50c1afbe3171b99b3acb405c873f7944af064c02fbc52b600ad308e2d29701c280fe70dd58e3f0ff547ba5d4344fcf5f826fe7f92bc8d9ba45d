            function walkTree(categoriesType, categoriesDef, parents) {
                if (parents === void 0) { parents = []; }
                var list = [];
                var tree = categoriesDef.map(function (categoryDef) {
                    var category = {
                        id: ++lastCategoryId,
                        type: categoriesType,
                        name: categoryDef.name,
                        slug: categoryDef.slug,
                        path: parents.map(function (x) { return x.slug; }).concat([categoryDef.slug]).join('/'),
                        image: categoryDef.image || null,
                        items: categoryDef.items || 0,
                        customFields: {},
                        parents: parents.slice(),
                        children: [],
                    };
                    var _a = walkTree(categoriesType, categoryDef.children || [], parents.concat([category])), childrenTree = _a[0], childrenList = _a[1];
                    category.children = childrenTree;
                    list = list.concat([category], childrenList);
                    return category;
                });
                return [tree, list];
            }
            var _a = walkTree('shop', shopCategoriesDef), shopCategoriesTree = _a[0], shopCategoriesList = _a[1];
            var _b = walkTree('blog', blogCategoriesDef), blogCategoriesTree = _b[0], blogCategoriesList = _b[1];
            function limitDepth(categories, depth) {
                return categories.map(function (category) {
                    return Object.assign({}, category, { parents: null, children: depth !== 0 ? limitDepth(category.children, depth - 1) : null });
                });
            }
            function getCategoriesTree(categoriesType, parentSlug, depth) {
                if (parentSlug === void 0) { parentSlug = null; }
                if (depth === void 0) { depth = 0; }
                var categories = [];
                var list = categoriesType === 'shop' ? shopCategoriesList : blogCategoriesList;
                var tree = categoriesType === 'shop' ? shopCategoriesTree : blogCategoriesTree;
                if (parentSlug === null) {
                    categories = tree.slice();
