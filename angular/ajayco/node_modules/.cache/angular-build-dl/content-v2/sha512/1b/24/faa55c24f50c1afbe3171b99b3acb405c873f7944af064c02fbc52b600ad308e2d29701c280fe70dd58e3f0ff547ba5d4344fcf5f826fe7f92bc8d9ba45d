                        Object(rxjs__WEBPACK_IMPORTED_MODULE_2__["fromEvent"])(_this.element, 'transitionend').pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_3__["takeUntil"])(_this.destroy$)).subscribe(function (event) {
